// this is setting for FTM
usingDetailedTransportModel  true;   //newly added
usingCoTHERM                 true;   //newly added

// Set major species list for CoTHERM
majorSpeciesForCoTHERM (CH4 O2 CO2 H2O);  //newly added

// Set threshold for CoTHERM
epsilonT          0.2;   // [K], newly added
epsilonP          100;   // [Pa], newly added
epsilonS          0.001; // [-], in mass fraction, newly added

thermoType
{
    type            hePsiThermo;
    mixture         FTMMulticomponentMixture;
    transport       FTMTransport;
    thermo          janaf;
    energy          sensibleEnthalpy;
    equationOfState perfectGas;
    specie          specie;
}

defaultSpecie N2;

#include "thermo.FTM"
// end of setting for FTM


